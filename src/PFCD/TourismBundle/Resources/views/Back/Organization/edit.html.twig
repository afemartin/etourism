{% extends 'PFCDTourismBundle:Back:layout.html.twig' %}

{% block body %}
<h1>Organization edit</h1>

<form action="{{ path('back_organization_update') }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button type="submit">Edit</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('back_organization_show') }}">
            Back to the profile
        </a>
    </li>
</ul>
{% endblock %}