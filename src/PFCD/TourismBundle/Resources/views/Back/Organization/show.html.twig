{% extends 'PFCDTourismBundle:Back:layout.html.twig' %}

{% block body %}
<h1>Organization</h1>

<table class="table table-striped table-bordered table-condensed">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Username</th>
            <td>{{ entity.username }}</td>
        </tr>
        <tr>
            <th>Password</th>
            <td>{{ entity.password }}</td>
        </tr>
        <tr>
            <th>Salt</th>
            <td>{{ entity.salt }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ entity.email }}</td>
        </tr>
        <tr>
            <th>Phone</th>
            <td>{{ entity.phone }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Acronim</th>
            <td>{{ entity.acronim }}</td>
        </tr>
        <tr>
            <th>Shortdesc</th>
            <td>{{ entity.shortDesc }}</td>
        </tr>
        <tr>
            <th>Fulldesc</th>
            <td>{{ entity.fullDesc }}</td>
        </tr>
        <tr>
            <th>Postalcode</th>
            <td>{{ entity.postalCode }}</td>
        </tr>
        <tr>
            <th>City</th>
            <td>{{ entity.city }}</td>
        </tr>
        <tr>
            <th>Locale</th>
            <td>{{ entity.locale }}</td>
        </tr>
        <tr>
            <th>Foundationyear</th>
            <td>{{ entity.foundationYear }}</td>
        </tr>
        <tr>
            <th>Geolocation</th>
            <td>{{ entity.geolocation }}</td>
        </tr>
        <tr>
            <th>Created</th>
            <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Updated</th>
            <td>{{ entity.updated|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Status</th>
            <td>{{ entity.status }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('back_organization_edit') }}">
            Edit
        </a>
    </li>
</ul>
{% endblock %}